{
  "name": "chastephp/laravel-wheres",
  "description": "A laravel wheres. Inspired by medoo ",
  "keywords": [
    "laravel",
    "laravel-query",
    "laravel-where"
  ],
  "license": "MIT",
  "require": {
    "php": ">7.1.3",
    "illuminate/database": ">6.9"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0",
    "phpunit/phpunit": "^8.0|^9.3"
  },
  "autoload": {
    "psr-4": {
      "ChastePhp\\LaravelWheres\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ChastePhp\\LaravelWheres\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ChastePhp\\LaravelWheres\\ServiceProvider"
      ]
    }
  },
  "authors": [
    {
      "name": "widdy",
      "email": "kingwide@foxmail.com"
    }
  ]
}
